<Window x:Class="WpfApplicationLogin.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplicationLogin"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="500">
    <Grid Background="#FF797871">
        <Button Content="New" HorizontalAlignment="Left" Margin="334,244,0,0" VerticalAlignment="Top" Width="143" Height="44" Background="White" FontSize="22" FontWeight="Bold" Click="New"/>
        <Button Content="Edit" HorizontalAlignment="Left" Margin="43,244,0,0" VerticalAlignment="Top" Width="130" Height="44" Background="White" FontSize="22" FontWeight="Bold" Click="Edit"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="192,244,0,0" VerticalAlignment="Top" Width="129" Height="44" Background="White" FontSize="22" FontWeight="Bold" Click="Delete"/>
        <ListBox DisplayMemberPath="logs" x:Name="liste" HorizontalAlignment="Left" Height="229" Margin="10,10,0,0" VerticalAlignment="Top" Width="467" SelectionChanged="ListBox_SelectionChanged">
        </ListBox>
    </Grid>
</Window>
